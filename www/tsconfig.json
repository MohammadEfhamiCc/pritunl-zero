{
  "compilerOptions": {
    "target": "es2018",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
